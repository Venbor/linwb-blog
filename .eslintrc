{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": 'babel-eslint',
  "extends": 'eslint-config-airbnb',
  "installedESLint": true,
  "plugins": [
    'html'
  ],
  "rules": {
    "strict": 0,
    "no-useless-escape": 0,
    "padded-blocks": 0,
    "class-methods-use-this": 0,
    "no-prototype-builtins":0,
    "max-len":[1,150],
    "global-require": 0,
    "import/no-dynamic-require": 1,
    "no-multi-assign": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "consistent-return": 1,
    "no-restricted-syntax": 0,
    "no-loop-func": 1,
    "no-plusplus":0,
    "comma-dangle": 1,
    "eqeqeq": 1,
    "func-names": 0,
    "no-mixed-operators": 0,
    "no-empty": 1,
    "import/no-extraneous-dependencies": 1,
    "object-shorthand": 0,
    "import/extensions": 0,
    "no-bitwise": 0,
    "no-console": [2, { "allow": ["warn", "error"] }]
  }
}
